{
	"overrides": [
		{
			"files": ["*.ts", "*.tsx"],
			"extends": ["@oly_op/eslint-config", "@oly_op/eslint-config-react"],
			"parserOptions": {
				"project": "./tsconfig.json"
			}
		},
		{
			"files": ["*.graphql", "*.gql", "*.graphqls"],
			"parser": "@graphql-eslint/eslint-plugin",
			"plugins": ["@graphql-eslint"],
			"extends": "@oly_op/eslint-config-graphql"
		}
	]
}
